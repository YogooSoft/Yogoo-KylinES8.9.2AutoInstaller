# ATTENTION: the file is generated by ansible, DO NOT modify it manually

server.name: {{ node_name }}

{% if server_port is defined and config['server.port'] is undefined and config['server']['port'] is undefined %}
server.port: {{ server_port }}
{% endif %}
{% if server_host is defined and config['server.host'] is undefined and config['server']['host'] is undefined %}
server.host: {{ server_host }}
{% endif %}

{% if version is version('7.0.0', '<') and es_hosts is defined and config['elasticsearch.url'] is undefined and config['elasticsearch']['url'] is undefined %}
elasticsearch.url: "{{ es_hosts | first }}"
{% elif es_hosts is defined and config['elasticsearch.hosts'] is undefined and config['elasticsearch']['hosts'] is undefined %}
elasticsearch.hosts: {{ es_hosts | unique | to_json }}
{% endif %}
{% if kibana_username is defined and config['elasticsearch.username'] is undefined and config['elasticsearch']['username'] is undefined %}
elasticsearch.username: "{{ kibana_username }}"
{% endif %}
{% if kibana_password is defined and config['elasticsearch.password'] is undefined and config['elasticsearch']['password'] is undefined %}
elasticsearch.password: "{{ kibana_password }}"
{% endif %}

xpack.encryptedSavedObjects.encryptionKey: e54000f417624dea2a7980dc894f5e29
xpack.security.encryptionKey: bba41270908c04b66f972a13d67e33a4

{% if xpack|bool %}
xpack.security.enabled: {{ 'true' if xpack_security|bool else 'false' }}
xpack.reporting.encryptionKey: {{ reporting_encryption_key }}
{% endif %}

{% if config %}
{{ config | to_nice_yaml(indent=2) }}
{% endif %}
